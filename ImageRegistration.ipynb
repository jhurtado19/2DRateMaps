      "source": [
        "## Show the mapping between the 3 source points and the 3 target points\n",
        "source_points = [(12,16),(7,4),(25,20)]\n",
        "target_points = [(15,22),(1,22),(25,10)]\n",
        "check_results(source_img, target_img, points1 = source_points, points2 = target_points)"
      ]
    },
    {
